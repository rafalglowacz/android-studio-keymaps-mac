<keymap version="1" name="My keymap" parent="Sublime Text (Mac OS X)">
  <action id="$Paste">
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="ChangesView.Move" />
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="EditorMoveToPageBottom" />
  <action id="EditorMoveToPageTop" />
  <action id="EditorPaste">
    <keyboard-shortcut first-keystroke="meta v" />
  </action>
  <action id="EditorPasteSimple">
    <keyboard-shortcut first-keystroke="shift ctrl alt v" />
  </action>
  <action id="EditorSelectWord">
    <keyboard-shortcut first-keystroke="meta l" />
    <keyboard-shortcut first-keystroke="shift meta j" />
    <keyboard-shortcut first-keystroke="shift meta space" />
    <keyboard-shortcut first-keystroke="shift ctrl m" />
  </action>
  <action id="EditorUnindentSelection">
    <keyboard-shortcut first-keystroke="meta open_bracket" />
    <keyboard-shortcut first-keystroke="shift tab" />
  </action>
  <action id="ManageRecentProjects">
    <keyboard-shortcut first-keystroke="shift ctrl alt p" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="shift meta close_bracket" />
    <keyboard-shortcut first-keystroke="meta alt right" />
    <keyboard-shortcut first-keystroke="ctrl page_down" />
    <keyboard-shortcut first-keystroke="meta page_down" />
  </action>
  <action id="PasteMultiple">
    <keyboard-shortcut first-keystroke="meta k" second-keystroke="meta v" />
    <keyboard-shortcut first-keystroke="meta k" second-keystroke="v" />
    <keyboard-shortcut first-keystroke="meta alt v" />
    <keyboard-shortcut first-keystroke="shift meta v" />
  </action>
  <action id="PrevParameter" />
  <action id="PrevTemplateParameter" />
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="shift meta open_bracket" />
    <keyboard-shortcut first-keystroke="meta alt left" />
    <keyboard-shortcut first-keystroke="ctrl page_up" />
    <keyboard-shortcut first-keystroke="meta page_up" />
  </action>
  <action id="PreviousTemplateVariable" />
  <action id="QuickChangeScheme" />
  <action id="Run">
    <keyboard-shortcut first-keystroke="shift f10" />
  </action>
  <action id="SearchEverywhere.PrevTab" />
  <action id="Stop">
    <keyboard-shortcut first-keystroke="meta f2" />
  </action>
  <action id="ToggleBookmark">
    <keyboard-shortcut first-keystroke="shift ctrl r" />
  </action>
  <action id="Vcs.MoveChangedLinesToChangelist" />
  <action id="VcsGroups">
    <keyboard-shortcut first-keystroke="alt g" />
  </action>
  <action id="android.deploy.ApplyChanges">
    <keyboard-shortcut first-keystroke="ctrl meta e" />
  </action>
  <action id="android.deploy.CodeSwap">
    <keyboard-shortcut first-keystroke="shift ctrl meta e" />
  </action>
</keymap>